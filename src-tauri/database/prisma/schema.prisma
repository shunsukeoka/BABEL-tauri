datasource db {
    provider = "sqlite"
    url      = "file:dev.db"
}

generator client {
    provider = "cargo prisma"
    output   = "../../src/db.rs"
}

model RootDirectory {
    id        Int      @id @default(autoincrement())
    path      String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
