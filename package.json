{
    "name": "babel",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "tauri": "tauri",
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "prettier": "prettier --write --ignore-path .gitignore --plugin-search-dir=. \"**/*.{json,yml,css,scss,js,jsx,ts,tsx,cjs}\"",
        "check-format": "npm run prettier -- --list-different",
        "lint": "eslint --ext \".js,.jsx,.ts,.tsx,.cjs\" --ignore-path .gitignore .",
        "lint:fix": "npm run lint -- --fix",
        "stylelint": "stylelint \"**/*.{css,scss,jsx,tsx}\" --ignore-path .gitignore",
        "stylelint:fix": "npm run stylelint -- --fix",
        "fix": "npm-run-all -s prettier lint:fix stylelint:fix",
        "test": "jest --config ./jest.config.json",
        "test:coverage": "npm run test -- --coverage",
        "test:watch": "npm run test -- --watch",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "generate": "plop",
        "prisma:migrate": "cd src-tauri/database && cargo prisma migrate dev",
        "prisma:run": "cd src-tauri/database && cargo prisma db push",
        "prepare": "husky install",
        "lint-staged": "lint-staged",
        "commitlint": "commitlint",
        "devtools": "npx react-devtools"
    },
    "lint-staged": {
        "*.{json,css,scss,js,jsx,ts,tsx,cjs}": [
            "npm run prettier"
        ],
        "*.{js,jsx,ts,tsx,cjs}": [
            "npm run lint:fix"
        ],
        "*.{css,scss,jsx,tsx}": [
            "npm run stylelint:fix"
        ]
    },
    "dependencies": {
        "@radix-ui/react-popover": "1.0.2",
        "@radix-ui/react-slider": "^1.1.0",
        "@rspc/client": "^0.1.2",
        "@rspc/tauri": "^0.1.2",
        "@stitches/react": "^1.2.8",
        "@storybook/react": "^6.5.14",
        "@tanstack/react-query": "^4.22.0",
        "@tauri-apps/api": "^1.2.0",
        "clsx": "^1.2.1",
        "date-fns": "^2.29.3",
        "neverthrow": "^6.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-icons": "^4.6.0",
        "react-router": "^6.6.2",
        "react-router-dom": "^6.6.2",
        "react-window": "^1.8.7",
        "reflect-metadata": "^0.1.13",
        "stitches-reset": "^1.0.0",
        "tsyringe": "^4.7.0",
        "zustand": "^4.3.1"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@commitlint/cli": "^17.4.1",
        "@commitlint/config-conventional": "^17.4.2",
        "@storybook/addon-actions": "^6.5.10",
        "@storybook/addon-essentials": "^6.5.14",
        "@storybook/addon-interactions": "^6.5.12",
        "@storybook/addon-links": "^6.5.12",
        "@storybook/builder-vite": "^0.2.7",
        "@storybook/testing-library": "^0.0.13",
        "@stylelint/postcss-css-in-js": "^0.38.0",
        "@tauri-apps/cli": "^1.2.2",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^12.1.5",
        "@testing-library/react-hooks": "^7.0.2",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^27.5.2",
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.12",
        "@types/react-dom": "^18.0.5",
        "@types/styled-components": "^5.1.26",
        "@types/testing-library__jest-dom": "^5.14.5",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "@typescript-eslint/parser": "^5.48.1",
        "@vitejs/plugin-react": "^3.0.1",
        "autoprefixer": "^10.4.12",
        "babel-loader": "^9.1.2",
        "eslint": "^8.31.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-jest-dom": "^4.0.2",
        "eslint-plugin-jsx-a11y": "^6.7.0",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.6.10",
        "eslint-plugin-testing-library": "^5.7.2",
        "husky": "^8.0.3",
        "jest": "^27.5.1",
        "lint-staged": "^13.0.3",
        "npm-run-all": "^4.1.5",
        "plop": "^3.1.1",
        "prettier": "^2.8.2",
        "sass": "^1.55.0",
        "stylelint": "^14.16.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recess-order": "^3.0.0",
        "stylelint-config-standard": "^29.0.0",
        "stylelint-config-standard-scss": "^6.1.0",
        "stylelint-prettier": "^2.0.0",
        "stylelint-processor-styled-components": "^1.10.0",
        "stylelint-scss": "^4.3.0",
        "ts-jest": "^27.1.5",
        "typescript": "^4.9.4",
        "vite": "^4.0.4"
    }
}
